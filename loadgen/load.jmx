        <hashTree>
          <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="Set Customer header" enabled="true">
            <boolProp name="resetInterpreter">true</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">import org.apache.jmeter.util.JMeterUtils;
import org.apache.jmeter.protocol.http.control.HeaderManager;
import java.util.concurrent.ThreadLocalRandom;
import java.io;
import java.util;

// set a random customer ID
String customerId = String.valueOf(ThreadLocalRandom.current().nextInt(1,1000000));

// set the customer type using a random weighted mix
String customerType = &quot;&quot;;
Integer randValue = ThreadLocalRandom.current().nextInt(1,100);
if (randValue &lt; 10) {
	customerType = &quot;gold&quot;;
}
else if (randValue &lt; 30) {
	customerType = &quot;silver&quot;;
}
else {
	customerType = &quot;bronze&quot;;
}

// Set header
// -------------------------------------------
HeaderManager hm = sampler.getHeaderManager();
hm.removeHeaderNamed(&quot;x-dynatrace-customer&quot;);
String headerValue = &quot;customerType=&quot; + customerType + &quot;;customerId=&quot; + customerId + &quot;;&quot;;
hm.add(new org.apache.jmeter.protocol.http.control.Header(&quot;x-dynatrace-customer&quot;, headerValue));</stringProp>
          </BeanShellPreProcessor>
